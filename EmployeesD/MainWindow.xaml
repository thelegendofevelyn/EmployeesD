<Window x:Class="EmployeesD.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EmployeesD"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Background="#FFF1F107">
        <TextBox Name ="Assets"  HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Height="399" Margin="180,10,0,0" Width="189" Background="White" Text="Assets Text" TextChanged="Assets_TextChanged">
        </TextBox>
        <TextBox Name ="Employees"  HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" Height="399" Margin="398,10,0,0" Width="188" Background="White" Text="Employees Text" TextChanged="Assets_TextChanged">
        </TextBox>
        <Button Content="See Assets" Margin="10,54,692,336" Click="Assets_Click"/>
        <Button Content="See Employees" Margin="10,88,692,302" Click="Employees_Click"/>
    </Grid>
</Window>
